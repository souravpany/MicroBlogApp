<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="5dp"    android:elevation="5dp"    android:orientation="vertical"    app:cardCornerRadius="5dp">    <TextView        android:id="@+id/txtTitle"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:maxLines="1"        android:textColor="@color/dot_dark_screen3"        android:textSize="17sp"        android:layout_margin="3dp"        app:fontFamily="@font/roboto_regular" /></androidx.cardview.widget.CardView>